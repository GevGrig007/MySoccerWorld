@model IEnumerable<Match>
<div>
    <div>
        @if (Model.Any(m => m.Round == "1/64 finals"))
        {
            <div class="round" style="background-color:darkcyan; text-align:center; font-size:24px; color:white">
                1/64   FINAL
            </div>
        }
        @foreach (var m in Model.Where(m => m.Round == "1/64 finals"))
        {

            if (m != null)
            {
                <div class="container">
                    @await Html.PartialAsync("Match", m)
                </div>
            }
        }
